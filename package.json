{
  "name": "babelfish-lookingglass",
  "version": "1.0.0",
  "description": "Babelfish server for Looking Glass project - Unifi Protect API explorer and broker",
  "main": "server.js",
  "scripts": {
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:discovery": "node test-auto-discovery.js",
    "test:mqtt": "node test-mqtt-flow.js",
    "test:telegram": "node test-telegram-connector.js",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "setup": "node setup.js",
    "health": "curl http://localhost:3000/health",
    "metrics": "curl http://localhost:3000/health/metrics"
  },
  "keywords": [
    "unifi",
    "protect",
    "api",
    "mqtt",
    "broker",
    "looking-glass",
    "babelfish"
  ],
  "author": "Looking Glass Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "kill-port": "^2.0.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mqtt": "^5.1.0",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
